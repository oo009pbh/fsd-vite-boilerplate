{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "es5",
    "strict": true,
    "resolveJsonModule": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@app": [
        "src/app"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@shared": [
        "src/shared"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@routes": [
        "src/routes"
      ],
      "@atom/*": [
        "src/shared/component/atom/*"
      ],
      "@atom": [
        "src/shared/component/atom"
      ],
      "@molecule/*": [
        "src/shared/component/molecule/*"
      ],
      "@molecule": [
        "src/shared/component/molecule"
      ],
      "@organisms/*": [
        "src/shared/component/organisms/*"
      ],
      "@organisms": [
        "src/shared/component/organisms"
      ],
      "@template/*": [
        "src/shared/component/template/*"
      ],
      "@typings/*": [
        "src/typings/*"
      ],
      "@typings": [
        "src/typings"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "global.d.ts",
    "svg.d.ts",
    "vite-env.d.ts",
    "src"
  ],
  "types": [
    "vite-plugin-svgr/client"
  ],
  "exclude": [
    "node_modules"
  ]
}
