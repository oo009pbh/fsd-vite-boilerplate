{
  "name": "payhere-calendar",
  "version": "0.1.0",
  "description": "박병훈의 프론트엔드 소스코드입니다.",
  "repository": {
    "type": "git",
    "url": "https://github.com/oo009pbh/payhere-assignment"
  },
  "private": true,
  "scripts": {
    "run:dev": "vite",
    "run:prod": "cross-env NODE_ENV=production vite --mode production",
    "build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:prod": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "build-storybook": "storybook build",
    "lint-staged": "lint-staged",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:error": "eslint --quiet \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "postinstall": "cd ../../ && husky install client/service/.husky",
    "generate-types": "openapi-typescript http://localhost:11001/swagger-json --output src/typings/api/schema/index.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "license": "ISC",
  "author": "oo009pbh",
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "animate.css": "^4.1.1",
    "axios": "^1.1.3",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "intl": "^1.2.5",
    "katex": "^0.16.9",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.12",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-select-search": "^4.1.7",
    "react-spinners": "^0.13.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/eslint-plugin": "^11.11.0",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-vite": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "openapi-typescript": "^6.7.0",
    "prettier": "^3.2.5",
    "rollup": "^3.21.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "storybook": "7.3.2",
    "style-loader": "3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.0",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "web-vitals": "^3.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
